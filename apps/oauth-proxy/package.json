    "build": "tsc --noEmit",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5",
    "vercel": "^39"
  },
  "dependencies": {
    "hono": "^4.6.0"
  }
}
