{
  "name": "@sentry/oauth-proxy",
  "version": "0.1.0",
  "description": "OAuth proxy server for Sentry CLI device flow",
  "type": "module",
  "scripts": {
    "dev": "bun run --env-file=../../.env dev.ts",
    "dev:3000": "PORT=3000 bun run --env-file=../../.env dev.ts",
